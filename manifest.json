{
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "version": "0.1.0",
  "default_locale": "en",
  "description": "__MSG_extDescription__",
  "action": {
    "default_title": "__MSG_actionTitle__"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage"],
  "host_permissions": ["https://whois.freeaiapi.xyz/*"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
